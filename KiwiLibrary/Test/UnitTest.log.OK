class Operation
{
	constructor(){
	}

	get(command){
		console.log("Operation.get(" + command + ");\n") ;
		return null ;
	}

	set(command, value){
		console.log("Operation.set(" + command + ", " + value + ");\n") ;
	}

	execute(){
		try {
			this.main() ;
		} catch(err){
			return err.code
		}
	}

	main(){
		console.log("Operation.main()\n") ;
		return 0 ;
	}

	cancel(){
		_cancel() ;
	}

	get core() {
		return _operation_core ;
	}
}

